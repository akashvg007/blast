{
  "expo": {
    "name": "Blast",
    "slug": "blast",
    "version": "1.0.19",
    "orientation": "portrait",
    "icon": "./assets/logo2.png",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Blast to access your photos",
          "cameraPermission":"Allow Blast to access your camera",
          "microphonePermission":"Allow Blast to access your microphone"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logo2.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/notification1.mp3",
            "./assets/notification.mp3"
          ],
          "mode": "production"
        }
      ]
    ],
    "splash": {
      "image": "./assets/logo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#111528"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.akashcorp.keepitblast",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo2.png",
        "backgroundColor": "#111528"
      },
      "permissions":["READ_CONTACTS","CAMERA","VIBRATE","ACCESS_BACKGROUND_LOCATION","RECEIVE_BOOT_COMPLETED"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}